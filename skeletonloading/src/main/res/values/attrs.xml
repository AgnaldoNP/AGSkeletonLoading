<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SkeletonView">
        <attr name="initWithLoading" format="boolean" />
        <attr name="enableDevelopPreview" format="boolean" />
        <attr name="skeletonColor" format="color" />
        <attr name="skeletonEffectStrokeWidth" format="dimension" />
        <attr name="skeletonEffectBlurWidth" format="dimension" />
        <attr name="skeletonEffectLightenFactor" format="float" />
        <attr name="skeletonCornerRadius" format="dimension" />

        <attr name="customCycleDuration" format="integer" />
        <attr name="cycleDuration" format="enum">
            <enum name="shortCycle" value="0" />
            <enum name="mediumCycle" value="1" />
            <enum name="longCycle" value="2" />
        </attr>
    </declare-styleable>

    <attr name="SkeletonView" format="reference" />

</resources>
